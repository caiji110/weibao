<view class="box">
  <video class="video" id="myVideo" autoplay="{{autoplay}}" initial-time="{{a}}"
    src='cloud://yiming-0g0pnjw80bcc3577.7969-yiming-0g0pnjw80bcc3577-1305623497/运动视频2（成品3）.mp4'
    objectFit='cover'></video>

  <view class="list">
    <view class="video-detail">
      <view class="title">晨起舒筋活络式\n</view>
      
      <view>动作步骤</view>
      <view class="line"></view>
    </view>
    <block wx:for="{{fitnessDetail}}">
      <view class="video-detail" bind:tap="{{item.eventId}}">

        <view>{{item.name}}</view>
        <view>{{item.timelong}}</view>
        <view class="line"></view>
      </view>

    </block>
  </view>
  <view class="button">

  <van-button bind:click="jump" class="button" size="large" round type="info"
    color="linear-gradient(to right, #55515E, #BFB7BB)">结束运动
  </van-button>
</view>
<view class="patch"></view>

</view>